-- Script for Tabs

Button1.MouseButton1Click:Connect(function()
                 SectionButton1.Visible = true
                 SectionButton2.Visible = true
                 SectionToggle1.Visible = true
                 SectionToggle2.Visible = true
                 SectionPlayer1.Visible = false
                 SectionPlayer2.Visible = false
                 SectionPlayer3.Visible = false
    end)

SectionButton1.MouseButton1Click:Connect(function()
    end)

SectionButton2.MouseButton1Click:Connect(function()
    end)

SectionToggle1.MouseButton1Click:Connect(function()
        Toggled.Visible = true
        SectionToggle1 = false
    end)

Toggled.MouseButton1Click:Connect(function()
        Toggled.Visible = false
        SectionToggle1.Visible = true
    end)

SectionToggle2.MouseButton1Click:Connect(function()
        Toggled2.Visible = true
        SectionToggle2 = false
    end)

Toggled2.MouseButton1Click:Connect(function()
        Toggled2.Visible = false
        SectionToggle2.Visible = true
    end)

